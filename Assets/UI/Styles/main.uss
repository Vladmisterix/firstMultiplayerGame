.main-panel {
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
    width: 100%;
    height: 100%;
    background-color: rgba(28, 190, 167, 255);
    align-items: center;
    justify-content: flex-start;
}

.label {
    font-size: 23px;
    -unity-font-style: bold;
    color: rgba(111, 84, 60, 255);
    margin-top: 50px;
}

.button {
    margin-top: 30px;
    width: 170px;
    height: 50px;
    font-size: 15px;
    -unity-font-style: normal;
}
